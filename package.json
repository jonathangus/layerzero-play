{
  "name": "web3-fullstack-starter",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "contracts:test": "turbo run test --scope=*smart-contracts*",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^2.8.4",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.17",
  "dependencies": {
    "@ethersproject/contracts": "^5.7.0",
    "@layerzerolabs/lz-sdk": "^0.0.12",
    "@wagmi/core": "^0.9.5",
    "eslint": "8.33.0",
    "ethers": "5.5.1",
    "wagmi": "0.11.5"
  }
}
